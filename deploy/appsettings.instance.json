{
  "Repository": "Oracle",
  "SupportedInteractions": {
    "InstanceLevelInteractions": "read, $validate, $export, $everything",
    "TypeLevelInteractions": "create, search, $validate, $export, $lastn",
    "WholeSystemInteractions": "capabilities, batch, transaction, history, search, compartment_system_search, $validate, $convert, $versions, $realworldtesting, $realworldtestingstatus, $export, $exportstatus, $exportfilerequest, $closure, $verify-integrity-status"
  },
  "SupportedModel": {
    "RestrictToResources": [ "Patient", "AuditEvent" ],
    "RestrictToSearchParameters": ["Resource._id", "Patient.gender", "Patient.birthdate", "StructureDefinition.url"]
  },
  "Audit": {
    "EnableAuditEventLogging": true,
    "EnableAuditFileLogging": true
  },
  "PipelineOptions": {
    "PluginDirectory": "./plugins",
    "Branches": [
      {
        "Path": "/",
        "Include": [
          "Vonk.Core",
          "Vonk.Plugin.Operations",
          "Vonk.Fhir.R4",
//          "Vonk.Subscriptions",
          "Vonk.Plugin.Smart",
          "Vonk.UI.Demo",
          "Vonk.Plugin.DocumentOperation.DocumentOperationConfiguration",
          "Vonk.Plugin.ConvertOperation.ConvertOperationConfiguration",
          "Vonk.Plugin.BinaryWrapper",
          "Vonk.Plugin.Audit",
          "Vonk.Plugin.SearchAnonymization",
          "Vonk.Plugins.Terminology",
          "Visi.Repository"
        ],
        "Exclude": [
          "Vonk.Subscriptions.Administration",
          "Vonk.Plugin.Audit.Integrity"
        ]
      },
      {
        "Path": "/administration",
        "Include": [
          "Vonk.Core",
          "Vonk.Fhir.R3",
          "Vonk.Fhir.R4",
          "Vonk.Repository.Sql.Raw.KAdminSearchConfiguration",
          "Vonk.Repository.Sqlite.SqliteAdministrationConfiguration",
          "Vonk.Repository.MongoDb.MongoDbAdminConfiguration",
          "Vonk.Repository.Memory.MemoryAdministrationConfiguration",
          "Vonk.Subscriptions.Administration",
          "Vonk.Plugins.Terminology",
          "Vonk.Administration",
          "Vonk.Plugin.BinaryWrapper"
        ]
      }
    ]
  }
}